*{margin: 0;padding: 0;font-style: normal;}
.clear{clear: both;}
html,body{font-size: 55%;font-family: "Microsoft YaHei", "SimSun", Helvetica, Arial, sans-serif;background: #f1f5f8;}
.box{width: 100%;height: 90%;}
.head{width: 100%;height:4.2rem;cursor: pointer;}
.head .prev,.head .next{width: 20%;text-align: center;float: left;line-height: 4.2rem;font-size:1.6rem;}
.head .tomon{width: 60%;float: left;text-align: center;line-height: 4.2rem;font-size: 1.6rem;}
.date ol{width: 100%;height: 4rem;background: #fff;padding: 0;}
.date ol li{display:block;float: left;width: 14.2857%;text-align: center;font-size: 1.6rem;color: #333;border: none;padding: 0;line-height: 4rem;height: 4rem;}
.date ul{width: 100%;display: flex;flex-direction: row;flex-wrap: wrap;}
.date ul li{display: block;width: 14.2857%;height: 4.5rem;text-align: center;font-size: 1.6rem;line-height:2rem;background: #fff;position: relative;}
.date ul li i{position: absolute;right: 0;bottom:0.3rem;display:block;height: 15px;line-height: 15px;width: 100%;text-align: center;color: #e35925;}
.date ul li span{display: block;width: 100%;height: 100%;text-align: center;}
.date ul .no_date{color: #999;line-height:2rem}
.date ul .act_wk{color: #e35925;}
.date ul .act_date{color: #000; height: 42px;weight:40px; border: 2px solid #19b5fe;border-radius: 25px}
.date ul .act_ds{background: #005bac;color: #fff;}

